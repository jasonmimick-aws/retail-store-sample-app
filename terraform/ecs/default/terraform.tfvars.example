environment_name = "retail-store-ecs"

# Datadog integration settings
enable_datadog = true
datadog_api_key = "your_datadog_api_key_here"
datadog_integration_role_name = "DatadogIntegrationRole"
# Replace with your actual Datadog Forwarder Lambda ARN
datadog_forwarder_lambda_arn = "arn:aws:lambda:us-east-1:YOUR_ACCOUNT_ID:function:datadog-forwarder"
